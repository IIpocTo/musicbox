{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {
  },
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-use-before-declare": false,
    "no-implicit-dependencies": [true, "dev"],
    "no-submodule-imports": [true, "semantic-ui-css"],
    "jsx-boolean-value": false
  },
  "rulesDirectory": []
}